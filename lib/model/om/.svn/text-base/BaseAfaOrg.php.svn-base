	 *
	 * @param      AfaLeg $l AfaLeg
	 * @return     void
	 * @throws     PropelException
	 */
	public function addAfaLeg(AfaLeg $l)
	{
		if ($this->collAfaLegs === null) {
			$this->initAfaLegs();
		}
		if (!in_array($l, $this->collAfaLegs, true)) { // only add it if the **same** object is not already associated
			array_push($this->collAfaLegs, $l);
			$l->setAfaOrg($this);
		}
	}


