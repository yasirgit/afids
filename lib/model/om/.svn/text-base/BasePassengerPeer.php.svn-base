		}

		$objs = null;
		if (empty($pks)) {
			$objs = array();
		} else {
			$criteria = new Criteria(PassengerPeer::DATABASE_NAME);
			$criteria->add(PassengerPeer::ID, $pks, Criteria::IN);
			$objs = PassengerPeer::doSelect($criteria, $con);
		}
		return $objs;
	}

} // BasePassengerPeer

