			$con->beginTransaction();
			$affectedRows += UserFilterPeer::doOnDeleteCascade(new Criteria(UserFilterPeer::DATABASE_NAME), $con);
			$affectedRows += BasePeer::doDeleteAll(UserFilterPeer::TABLE_NAME, $con);
