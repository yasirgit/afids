		if (!$this->alreadyInValidation) {
			$this->alreadyInValidation = true;
			$retval = null;

			$failureMap = array();


			// We call the validate method on the following object(s) if they
			// were passed to this object by their coresponding set
			// method.  This object relates to these object(s) by a
			// foreign key reference.

			if ($this->aMissionLeg !== null) {
				if (!$this->aMissionLeg->validate($columns)) {
					$failureMap = array_merge($failureMap, $this->aMissionLeg->getValidationFailures());
				}
			}

			if ($this->aAircraft !== null) {
				if (!$this->aAircraft->validate($columns)) {
					$failureMap = array_merge($failureMap, $this->aAircraft->getValidationFailures());
				}
			}

			if ($this->aAfaOrg !== null) {
				if (!$this->aAfaOrg->validate($columns)) {
					$failureMap = array_merge($failureMap, $this->aAfaOrg->getValidationFailures());
				}
			}


			if (($retval = AfaLegPeer::doValidate($this, $columns)) !== true) {
				$failureMap = array_merge($failureMap, $retval);
			}



			$this->alreadyInValidation = false;
		}

		return (!empty($failureMap) ? $failureMap : true);
	}

