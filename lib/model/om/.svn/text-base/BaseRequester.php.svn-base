	public function clearAllReferences($deep = false)
	{
		if ($deep) {
			if ($this->collMissionsRelatedByRequesterId) {
				foreach ((array) $this->collMissionsRelatedByRequesterId as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collMissionsRelatedByOtherRequesterId) {
				foreach ((array) $this->collMissionsRelatedByOtherRequesterId as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collPassengers) {
				foreach ((array) $this->collPassengers as $o) {
					$o->clearAllReferences($deep);
				}
			}
		} // if ($deep)

		$this->collMissionsRelatedByRequesterId = null;
		$this->collMissionsRelatedByOtherRequesterId = null;
		$this->collPassengers = null;
