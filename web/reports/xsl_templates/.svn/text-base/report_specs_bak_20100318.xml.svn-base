		<report>
			<reportName>cancelled_mission_detail</reportName>
			<title>Cancelled Mission Detail</title>
			<category>Statistics</category>
			<category_2></category_2>
			<category_3></category_3>
			<active>true</active>
			<descriptionText></descriptionText>
			<security>
				<userRights>
					<userRight>REPNV</userRight>
				</userRights>
			</security>
			<dataset commandText="SELECT year(m.mission_date) as missionYear, month(m.mission_date) as monthNo, cancelled, count(ml.id) as legCount FROM mission m JOIN mission_leg ml ON (m.id = ml.mission_id)" commandType='adCmdText' commandTimeout='60'>
				<inputParameters>
					<inputParameter paramName='year' fieldName='year(m.mission_date)' fieldDesc='Year' operator='eq' required='yes' useroption='yes' bottomoption='yes' default='currentyear' datatype='int' fieldType='text'  fieldSize='10' />
				</inputParameters>
				<groupByFields>
					<groupByField>month(m.mission_date)</groupByField>
					<groupByField>cancelled</groupByField>
				</groupByFields>
			</dataset>
			<pageSize>0</pageSize>
			<outputformats>
				<outputformat type='html' description='Display' primary='yes' stylesheet='cancelledMissions.xsl' newwindow='no' templateOption='afidFrame'/>
				<outputformat type='chart' description='Display' primary='no' format='graph' visualization='intensitymap' newwindow='no' templateOption='afidFrame'>
					<column field="month" label="Month" width="20%" dataType="int" align="left" class=""/>
					<column field="cancelled" label="Cancelled" width="30%" dataType="int" align="left" class=""/>
					<column field="mission_leg" label="Legs" width="30%" dataType="int" align="left" class=""/>
					<column field="coordinated" label="Coordinated" width="30%" dataType="int" align="left" class=""/>
				</outputformat>
				<outputformat type='print' description='Print' primary='no' stylesheet='afwPrintReport.xsl' newwindow='no' templateOption='printFrame'>
				</outputformat>
			</outputformats>
		</report>
		<report>
			<reportName>cancelled_mission_detail_wing</reportName>
			<title>Cancelled Mission Detail by Wing</title>
			<category>Statistics</category>
			<category_2></category_2>
			<category_3></category_3>
			<active>true</active>
			<descriptionText></descriptionText>
			<security>
				<userRights>
					<userRight>REPNV</userRight>
				</userRights>
			</security>
			<dataset commandText="SELECT year(m.mission_date) as missionYear, month(m.mission_date) as monthNo, wing.name, count(ml.id) as legCount FROM mission m JOIN mission_leg ml ON (m.id = ml.mission_id) JOIN member mb ON (ml.pilot_id = mb.id) JOIN wing ON (mb.wing_id = wing.id)" commandType='adCmdText' commandTimeout='60'>
				<inputParameters>
					<inputParameter paramName='year' fieldName='year(m.mission_date)' fieldDesc='Year' operator='eq' required='yes' useroption='yes' bottomoption='yes' default='currentyear' datatype='int' fieldType='text'  fieldSize='10' />
				</inputParameters>
				<groupByFields>
					<groupByField>month(m.mission_date)</groupByField>
					<groupByField>wing.name</groupByField>
				</groupByFields>
			</dataset>
			<pageSize>0</pageSize>
			<outputformats>
				<outputformat type='html' description='Display' primary='yes' stylesheet='cancelledMissions_wing.xsl' newwindow='no' templateOption='afidFrame'/>
				<outputformat type='chart' description='Display' primary='no' format='graph' visualization='intensitymap' newwindow='no' templateOption='afidFrame'>
					<column field="month" label="Month" width="20%" dataType="int" align="left" class=""/>
					<column field="cancelled" label="Cancelled" width="30%" dataType="int" align="left" class=""/>
					<column field="mission_leg" label="Legs" width="30%" dataType="int" align="left" class=""/>
					<column field="coordinated" label="Coordinated" width="30%" dataType="int" align="left" class=""/>
				</outputformat>
				<outputformat type='print' description='Print' primary='no' stylesheet='afwPrintReport.xsl' newwindow='no' templateOption='printFrame'>
				</outputformat>
			</outputformats>
		</report>
