					   <selectOption description='All' value='all' default='yes' />
					   <selectOption description='Not Available' value='1' default='no' />
					   <selectOption description='Available' value='0' default='no' />
					   <selectOption description='Available' value='0' default='no' />
					   <selectOption description='Available Weekdays' value='0' default='no' />
					   <selectOption description='Available Weekends' value='0' default='no' />
