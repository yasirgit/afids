		<report>
			<reportName>alaskaAirlines</reportName>
			<title>Alaska Airlines Report</title>
			<descriptionText></descriptionText>
			<category>Missions</category>
			<category_2></category_2>
			<category_3></category_3>
			<active>true</active>
			<security>
				<userRights>
					<userRight>REPNV</userRight>
				</userRights>
			</security>
			<dataset commandText='SELECT mr.mission_date, concat_ws(" ", p.first_name,p.last_name) as passengerName, fa.ident as origin, ta.ident as destination, (case when (mr.airline_ref_number is Not Null) then mr.airline_ref_number else "" END) as VTA, (case when (`pa`.`illness` is null) then "" else pa.illness end) AS `illness` FROM mission m JOIN mission_leg ml on (m.id = ml.mission_id) JOIN passenger pa on (m.passenger_id = pa.id) JOIN person p on (pa.person_id = p.id) JOIN mission_report mr on (ml.mission_report_id = mr.id) JOIN airport fa on (ml.from_airport_id = fa.id) JOIN airport ta on (ml.to_airport_id = ta.id) WHERE pilot_id = 4028' commandType='adCmdText' commandTimeout='60'>
				<inputParameters>
					<inputParameter paramName='startDate' fieldName='mr.mission_date' fieldDesc='Start Date' operator='ge' required='yes' useroption='yes' default='monthstart' datatype='date' fieldType='text'  fieldSize='10' />
					<inputParameter paramName='endDate' fieldName='mr.mission_date' fieldDesc='End Date' operator='le' required='yes' useroption='yes' default='monthend' datatype='date' fieldType='text'  fieldSize='10' />
				</inputParameters>
				<orderByFields>
					<orderByField type='ASC'>mr.mission_date</orderByField>
				</orderByFields>
			</dataset>
			<pageSize>25</pageSize>
			<outputformats>
				<outputformat type='html' description='Normal' primary='yes' stylesheet='afwHTMLReport.xsl' newwindow='no' templateOption='afidFrame' >
					<column field='mission_date' label='Mission Date' width='10%' dataType="date" align='left' class=""/>
					<column field='passengerName' label='Passenger Name' dataType="text" width='25%' align='left' class="" />
					<column field='origin' label='Origin' width='15%' dataType="text" align='left' class="" />
					<column field='destination' label='Destination' dataType="text" width='15%' align='left' class="" />
					<column field='VTA' label='VTA' width='10%' dataType="text" align='left' class="" />
					<column field='illness' label='Illness' width='25%' dataType="text" align='left' class="" />
				</outputformat>
				<outputformat type='print' description='Print' primary='no' stylesheet='afwHTMLPrintReport.xsl' newwindow='no' templateOption='printFrame'>
					<column field='mission_date' label='Mission Date' width='10%' align='left' />
					<column field='passengerName' label='Passenger Name' width='25%' align='left' />
					<column field='origin' label='Origin' width='15%' align='left' />
					<column field='destination' label='Destination' width='15%' align='left' />
					<column field='VTA' label='VTA' width='10%' align='left' />
					<column field='illness' label='Illness' width='25%' align='left' />
				</outputformat>
				<outputformat type='download' description='Download to Excel' primary='no' stylesheet='afwDownloadReport.xsl' newwindow='yes' templateOption='afidFrame' >
					<column field='missionDate' label='Mission Date' width='10%' align='left' />
					<column field='passengerName' label='Passenger Name' width='25%' align='left' />
					<column field='origin' label='Origin' width='15%' align='left' />
					<column field='destination' label='Destination' width='15%' align='left' />
					<column field='VTA' label='VTA' width='10%' align='left' />
					<column field='illness' label='Illness' width='25%' align='left' />
				</outputformat>
			</outputformats>
		</report>
